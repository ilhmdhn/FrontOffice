<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@+id/swipe"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_primary_background"
    android:paddingHorizontal="12dp"
    tools:context=".view.fragment.reporting.kas.statuskamar.ReportKasStatusKamarFragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TableRow
                android:layout_width="match_parent"
                android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Checkin Sudah Bayar "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_cin_paid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Jam Sudah Bayar "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_jam_paid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Tamu Sudah Bayar "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_guest_paid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>

                <TableRow
                android:layout_width="match_parent"
                android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Checkin  Piutang "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_cin_piutang"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>

                <TableRow
                android:layout_width="match_parent"
                android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Jam  Piutang "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_jam_piutang"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>

                <TableRow
                android:layout_width="match_parent"
                android:layout_marginVertical="4dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:fontFamily="sans-serif-medium"
                        android:text="Jumlah Tamu Piutang "
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="5dp"
                        android:fontFamily="sans-serif-medium"
                        android:text=":"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_guest_piutang"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:textColor="@color/dark_primary_text"
                        android:textSize="14sp"
                        tools:text="1" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>